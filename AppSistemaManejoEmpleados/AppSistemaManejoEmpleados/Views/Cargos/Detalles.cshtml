@model AppSistemaManejoEmpleados.Models.Cargo
@{
    ViewData["Title"] = "Detalles del Cargo";
}
<h1>@ViewData["Title"]</h1>
<div>
    <dl class="row">
        <dt class="col-sm-2">Título</dt>
        <dd class="col-sm-10">@Model.Titulo</dd>
    </dl>
</div>
<p>
    <a asp-action="Editar" asp-route-id="@Model.Id" class="btn btn-warning">Editar</a>
    <a asp-action="Index" class="btn btn-secondary">Volver</a>
</p>
